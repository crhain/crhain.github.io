{
  "name": "crhain.github.io",
  "version": "1.0.0",
  "description": "My Portfolio Website",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "browserslist": "last 10 versions",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/style.comp.css",
    "prefix:css": "postcss css/style.concat.css --use autoprefixer -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crhwebdev/crhwebdev.github.io.git"
  },
  "author": "Carl Hain",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/crhwebdev/crhwebdev.github.io.git/issues"
  },
  "homepage": "https://github.com/crhwebdev/crhwebdev.github.io.git#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "concat": "^1.0.3",
    "hoek": "^5.0.3",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^4.1.1"
  }
}
